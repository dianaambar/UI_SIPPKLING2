<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".KategoriPendataan">

    <android.support.v4.widget.NestedScrollView
        android:id="@+id/scrollContent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fitsSystemWindows="true"
        app:layout_constraintTop_toTopOf="parent"
        >

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="15dp"
            >
            <RelativeLayout
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/bagianPendataanUtama"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/pendataanUtama"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/pendataanutama"
                    android:textSize="@dimen/medium"
                    android:fontFamily="@font/gotham_medium"
                    />
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/pendataanUtama"
                    android:layout_margin="3dp"
                    >
                    <ImageView
                            android:contentDescription="@string/app_name"
                        android:layout_marginTop="5dp"
                        android:id="@+id/imgRumahSehat"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/rumahsehaticon"
                        />
                    <TextView
                        android:id="@+id/txtRumahSehat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/rumahsehat"
                        android:layout_below="@+id/imgRumahSehat"
                        android:textSize="@dimen/average"
                        android:layout_centerHorizontal="true"
                        android:fontFamily="@font/gotham_light"
                        />
                </RelativeLayout>

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/bagianTempatUmum"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/bagianPendataanUtama"
                android:layout_marginTop="20dp"
                android:gravity="center"

                >
                <TextView
                    android:id="@+id/tempatUmum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tempatumum"
                    android:textSize="@dimen/medium"
                    android:fontFamily="@font/gotham_medium"
                    />
                <LinearLayout
                    android:baselineAligned="false"
                    android:layout_below="@+id/tempatUmum"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <RelativeLayout
                            android:id="@+id/rumahSakit"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="3dp"
                            >
                            <ImageView
                                android:contentDescription="@string/app_name"
                                android:id="@+id/imgRumahSakit"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:src="@drawable/rumahsakit"
                                android:layout_centerHorizontal="true"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/rumahsakit"
                                android:layout_below="@+id/imgRumahSakit"
                                android:layout_centerHorizontal="true"
                                android:textSize="@dimen/small"
                                android:fontFamily="@font/gotham_light"
                                />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_margin="3dp"
                            android:id="@+id/puskesmas"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            >
                            <ImageView
                                android:id="@+id/imgPuskesmas"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:src="@drawable/puskesmas"
                                android:layout_centerHorizontal="true"
                                android:contentDescription="@string/app_name"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/puskesmas"
                                android:layout_below="@+id/imgPuskesmas"
                                android:layout_centerHorizontal="true"
                                android:textSize="@dimen/small"
                                android:fontFamily="@font/gotham_light"
                                />

                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/Klinik"
                            android:layout_margin="3dp"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            >
                            <ImageView
                                android:id="@+id/imgKlinik"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:src="@drawable/klinik"
                                android:layout_margin="2dp"
                                android:layout_centerHorizontal="true"
                                android:contentDescription="@string/app_name"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/klinik"
                                android:layout_below="@+id/imgKlinik"
                                android:layout_centerHorizontal="true"
                                android:textSize="@dimen/small"
                                android:fontFamily="@font/gotham_light"
                                />

                        </RelativeLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <RelativeLayout
                            android:layout_toEndOf="@+id/rumahSakit"
                            android:id="@+id/tempatIbadah"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="3dp"
                            tools:ignore="ObsoleteLayoutParam">
                            <ImageView
                                android:id="@+id/imgTempatIbadah"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:src="@drawable/tempatibadah"
                                android:layout_centerHorizontal="true"
                                android:contentDescription="@string/app_name"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/tempatibadah"
                                android:layout_below="@+id/imgTempatIbadah"
                                android:layout_centerHorizontal="true"
                                android:textSize="@dimen/small"
                                android:fontFamily="@font/gotham_light"
                                android:gravity="center"
                                />

                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/Pesantren"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="3dp"
                            >
                            <ImageView
                                android:id="@+id/imgPesantren"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:src="@drawable/pesantren"
                                android:layout_centerHorizontal="true"
                                android:contentDescription="@string/app_name"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/pesantren"
                                android:layout_below="@+id/imgPesantren"
                                android:layout_centerHorizontal="true"
                                android:textSize="@dimen/small"
                                android:fontFamily="@font/gotham_light"
                                />

                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/Hotel"
                            android:layout_margin="3dp"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            >
                            <ImageView
                                android:id="@+id/imgHotel"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:src="@drawable/hotel"
                                android:layout_margin="2dp"
                                android:layout_centerHorizontal="true"
                                android:contentDescription="@string/app_name"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/hotel"
                                android:layout_below="@+id/imgHotel"
                                android:layout_centerHorizontal="true"
                                android:textSize="@dimen/small"
                                android:fontFamily="@font/gotham_light"
                                />
                        </RelativeLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <RelativeLayout
                            android:id="@+id/sekolah"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="3dp"
                            >
                            <ImageView
                                android:id="@+id/imgSekolah"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:src="@drawable/sekolah"
                                android:layout_centerHorizontal="true"
                                android:contentDescription="@string/app_name"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/sekolah"
                                android:layout_below="@+id/imgSekolah"
                                android:layout_centerHorizontal="true"
                                android:textSize="@dimen/small"
                                android:fontFamily="@font/gotham_light"
                                />

                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/kolamRenang"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="3dp"
                            >
                            <ImageView
                                android:id="@+id/imgKolamRenang"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:src="@drawable/kolamrenang"
                                android:layout_centerHorizontal="true"
                                android:contentDescription="@string/app_name"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/kolamrenang"
                                android:layout_below="@+id/imgKolamRenang"
                                android:layout_centerHorizontal="true"
                                android:textSize="@dimen/small"
                                android:fontFamily="@font/gotham_light"
                                android:gravity="center"
                                />

                        </RelativeLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <RelativeLayout
                            android:id="@+id/Pasar"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="3dp"
                            >
                            <ImageView
                                android:id="@+id/imgPasar"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:src="@drawable/pasar"
                                android:layout_centerHorizontal="true"
                                android:contentDescription="@string/app_name"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/pasar"
                                android:layout_below="@+id/imgPasar"
                                android:layout_centerHorizontal="true"
                                android:textSize="@dimen/small"
                                android:fontFamily="@font/gotham_light"
                                />

                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/HotelMelati"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="3dp"
                            android:gravity="center"
                            >
                            <ImageView
                                android:id="@+id/imgHotelMelati"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:src="@drawable/hotelmelati"
                                android:layout_centerHorizontal="true"
                                android:contentDescription="@string/app_name"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/hotelmelati"
                                android:layout_below="@+id/imgHotelMelati"
                                android:layout_centerHorizontal="true"
                                android:textSize="@dimen/small"
                                android:gravity="center"
                                android:fontFamily="@font/gotham_light"
                                />

                        </RelativeLayout>

                    </LinearLayout>

                </LinearLayout>

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/bagianPelayanKesLing"
                app:layout_constraintTop_toBottomOf="@+id/bagianTempatUmum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="20dp"
                >
                <TextView
                    android:id="@+id/pelayanKesLIing"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/pelayanankesling"
                    android:textSize="@dimen/medium"
                    android:fontFamily="@font/gotham_medium"
                    />
                <RelativeLayout
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/pelayanKesLIing"
                    android:layout_margin="3dp"
                    android:gravity="center"
                    >
                    <ImageView
                        android:layout_marginTop="5dp"
                        android:id="@+id/imgKesling"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/kesehatanlingkungan"
                        android:contentDescription="@string/app_name"
                        />
                    <TextView
                        android:id="@+id/txtKesLing"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/kesling"
                        android:layout_below="@+id/imgKesling"
                        android:textSize="@dimen/average"
                        android:layout_centerHorizontal="true"
                        android:fontFamily="@font/gotham_light"
                        android:gravity="center"
                        />
                </RelativeLayout>

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/bagianTempatKelolaMakanan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/bagianPelayanKesLing"
                app:layout_constraintStart_toStartOf="parent"
                >
                <TextView
                    android:id="@+id/PengelolaanMakanan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tempatpengelolaanmakanan"
                    android:textSize="@dimen/medium"
                    android:fontFamily="@font/gotham_medium"
                    android:gravity="center"
                    android:layout_marginTop="20dp"
                    />
                <RelativeLayout
                    android:id="@+id/depotAirMinum"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/PengelolaanMakanan"
                    android:layout_margin="3dp"
                    >
                    <ImageView
                        android:layout_marginTop="5dp"
                        android:id="@+id/imgDepotAirMinum"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/depotairminum"
                        android:contentDescription="@string/app_name"
                        />
                    <TextView
                        android:id="@+id/txtDepotAirMinum"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/depotairminum"
                        android:layout_below="@+id/imgDepotAirMinum"
                        android:textSize="@dimen/average"
                        android:layout_centerHorizontal="true"
                        android:fontFamily="@font/gotham_light"
                        android:gravity="center"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/restoran"
                    android:layout_toEndOf="@+id/depotAirMinum"
                    android:layout_below="@+id/PengelolaanMakanan"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="3dp"
                    >
                    <ImageView
                        android:layout_marginTop="5dp"
                        android:id="@+id/imgRumahMakan"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/rumahmakanrestaurant"
                        android:contentDescription="@string/app_name"
                        />
                    <TextView
                        android:id="@+id/txtRestaurant"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/rumahmakan"
                        android:layout_below="@+id/imgRumahMakan"
                        android:textSize="@dimen/average"
                        android:layout_centerHorizontal="true"
                        android:fontFamily="@font/gotham_light"
                        android:gravity="center"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/jasaBoga"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/PengelolaanMakanan"
                    android:layout_toEndOf="@+id/restoran"
                    android:layout_margin="3dp"
                    >
                    <ImageView
                        android:layout_marginTop="5dp"
                        android:id="@+id/imgJasaBoga"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/pemeriksaanjasaboga"
                        android:contentDescription="@string/app_name"
                        />
                    <TextView
                        android:id="@+id/txtJasaBoga"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/jasaboga"
                        android:layout_below="@+id/imgJasaBoga"
                        android:textSize="@dimen/average"
                        android:layout_centerHorizontal="true"
                        android:fontFamily="@font/gotham_light"
                        android:gravity="center"
                        />
                </RelativeLayout>

            </RelativeLayout>

        </android.support.constraint.ConstraintLayout>
    </android.support.v4.widget.NestedScrollView>

</android.support.constraint.ConstraintLayout>